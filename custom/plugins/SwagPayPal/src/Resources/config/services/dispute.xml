           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="Swag\PayPal\Dispute\Administration\DisputeController" public="true">
            <argument type="service" id="Swag\PayPal\RestApi\V1\Resource\DisputeResource" />
            <call method="setContainer">
                <argument type="service" id="service_container"/>
            </call>
        </service>
    </services>
</container>
