{% block bf_sales_channel_defaults_select_only %}
    <sw-container class="sw-sales-channel-defaults-select">
        <template v-if="salesChannel">
            <sw-entity-single-select
                :disabled="disabled"
                :class="singleSelectClass"
                :label="defaultPropertyLabel"
                :helpText="helpText"
                required
                :entity="propertyEntityName"
                :value="defaultId"
                :error="defaultsValueError"
                @change="updateDefault">
            </sw-entity-single-select>
        </template>
    </sw-container>
{% endblock %}
